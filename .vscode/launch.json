      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": false,
      "env": {
        "DIPDUP_DEBUG": "1",
        "DIPDUP_NO_SYMLINK": "1"
      }
    },
    {
      "name": "hydration: init",
      "type": "debugpy",
      "request": "launch",
      "module": "dipdup",
      "args": [
        "-e",
        ".env",
        "-C",
        "hydration",
